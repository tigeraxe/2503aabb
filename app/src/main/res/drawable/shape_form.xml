<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="2dp"
                android:left="1.5dp"
                android:right="2dp"
                android:top="1.5dp"/>
            <solid android:color="#126e7d8b"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle"
            android:useLevel="true">
            <solid android:color="#12273744"/>
            <corners android:radius="10dp"/>
            <padding
                android:top="10dp"
                android:right="10dp"
                android:left="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>
</layer-list>